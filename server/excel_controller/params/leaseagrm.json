{
    "columns": 
    {
        "Agreement Number": 
        {
            "Data type": "Text",
            "Example": "37-U468-10Oct2020",
            "Requirement": "Compulsory"
        },
        "Tenant ID": 
        {
            "Data type": "ID",
            "Example": "37",
            "Requirement": "Compulsory"
        },
        "Unit": 
        {
            "Data type": "ID",
            "Example": "G01",
            "Requirement": "Compulsory"
        },
        "Start date": 
        {
            "Data type": "Date",
            "Example": "14/08/2020",
            "Requirement": "Compulsory"
        },
        "End date": 
        {
            "Data type": "Date",
            "Example": "05/12/2021",
            "Requirement": "Compulsory"
        },
        "Rent amount": 
        {
            "Data type": "Number",
            "Example": "12000000",
            "Requirement": "Compulsory"
        },
        "Deposit amount": 
        {
            "Data type": "Number",
            "Example": "12000000",
            "Requirement": "Optional"
        },
        "Deposit payment date": 
        {
            "Data type": "Date",
            "Example": "10/08/2020",
            "Requirement": "Optional"
        },
        "Deposit payback date": 
        {
            "Data type": "Date",
            "Example": "05/12/2021",
            "Requirement": "Compulsory"
        },
        "Monthly payment date": 
        {
            "Data type": "Number",
            "Example": "15,\n Default: 1",
            "Requirement": "Optional"
        },
        "Deposit currency": 
        {
            "Data type": "Text",
            "Example": " USD \n Default: VND",
            "Requirement": "Optional"
        },
        "Deposit exchange rate": 
        {
            "Data type": "Number",
            "Example": "23200 \n Default: 1",
            "Requirement": "Compulsory"
        },
        "Note": 
        {
            "Data type": "Text",
            "Example": " 2 bikes, 1 TV",
            "Requirement": "Optional"
        }
    }, 
    "dropdown_list": 
    {
        "Unit": 
        {
            "table": "unit",
            "selects": ["`name` AS `Units`"], 
            "building_id": true 
        }, 
        "Tenant ID": 
        {
            "table": "tenant", 
            "selects": 
            [
                "`id` AS `Tenant ID`", 
                "CONCAT(IFNULL(`Last_Name`, ''),', ', IFNULL(`Middle_Name`, ''), ', ', IFNULL(`First_Name`, '')) AS `Tenant Name`"
            ], 
            "building_id": true, 
            "details": true 
        }
    }
}


