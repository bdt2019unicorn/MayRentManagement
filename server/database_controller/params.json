{
    "buildings": 
    {
        "table": "buildings"
    }, 
    "expense": 
    {
        "table": "expense", 
        "date_collumns": ["start_period", "end_period", "payment_date"], 
        "get_id": 
        {
            "unit":  
            {
                "search": "name", 
                "change": "unit_id", 
                "table": "unit", 
                "find_by_building": true 
            }, 
            "expense_type":  
            {
                "search": "name", 
                "change": "expense_type_id", 
                "table": "expense_type"
            }
        },
        "comma": ["amount"]
    }, 
    "leaseagrm": 
    {
        "table": "leaseagrm", 
        "date_collumns": ["start_date", "end_date", "deposit_payment_date", "deposit_payback_date"], 
        "get_id": 
        {
            "unit": 
            {
                "search": "name", 
                "change": "unit_id", 
                "table": "unit", 
                "find_by_building": true 
            }
        },
        "comma": ["rent_amount","deposit_amount", "deposit_exchange_rate"], 
        "change": 
        {
            "agreement_number": "name", 
            "end_date": "finish" 
        }
    }, 
    "revenue": 
    {
        "table": "revenue", 
        "date_collumns": ["payment_date"], 
        "get_id": 
        {
            "lease_agreement":  
            {
                "search": "name", 
                "change": "leaseagrm_id", 
                "table": "leaseagrm"
            }
        },
        "comma": ["amount"]
    }, 
    "tenant": 
    {
        "table": "tenant",  
        "date_collumns": ["date_of_birth"], 
        "change": 
        {
            "expected_departure_date_(police)": "expected_departure_date", 
            "residential_registration_date_(police)": "police_registration_date"
        }, 
        "get_id": 
        {
            "building": 
            {
                "search": "name",
                "change": "building_id", 
                "table": "buildings"
            }
        }
    },     
    "unit": 
    {
        "table": "unit", 
        "get_id": 
        {
            "building": 
            {
                "search": "name",
                "change": "building_id", 
                "table": "buildings"
            }
        }, 
        "change": 
        {
            "bedroom_number": "number_of_bedrooms", 
            "bathroom_number": "number_of_bathroom", 
            "window_number": "number_of_windows" 
        }
    }, 
    "user": 
    {
        "table": "user", 
        "exclude_columns": ["email_confirm"]
    }, 
    "utilities": 
    {
        "table": "utility_reading", 
        "date_columns": ["date_enter"], 
        "get_id":  
        {
            "utility_type": 
            {
                "search": "name",
                "change": "revenue_type_id", 
                "table": "revenue_type"
            }, 
            "unit": 
            {
                "search": "name", 
                "change": "unit_id", 
                "table": "unit", 
                "find_by_building": true 
            }
        }
    }
}  

